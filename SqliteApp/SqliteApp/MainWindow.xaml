<Window x:Class="SqliteApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SqliteApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Add" Content="Add" HorizontalAlignment="Left" Margin="209,306,0,0" VerticalAlignment="Top" Width="75" Click="Add_Click"/>
        <TextBox x:Name="fistNametxt" HorizontalAlignment="Left" Height="32" Margin="209,74,0,0" TextWrapping="Wrap" Text="FirstName" VerticalAlignment="Top" Width="300"/>
        <TextBox x:Name="lastNametxt" HorizontalAlignment="Left" Height="29" Margin="209,111,0,0" TextWrapping="Wrap" Text="LastName" VerticalAlignment="Top" Width="300"/>
        <TextBox x:Name="emailtxt" HorizontalAlignment="Left" Height="30" Margin="209,145,0,0" TextWrapping="Wrap" Text="Email" VerticalAlignment="Top" Width="300"/>
        <Button x:Name="ShowAll" Content="ShowAll" HorizontalAlignment="Left" Margin="323,306,0,0" VerticalAlignment="Top" Width="75" Click="ShowAll_Click"/>

    </Grid>
</Window>
